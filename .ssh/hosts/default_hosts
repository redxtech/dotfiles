# defualt hosts for all computers

Host desktop
    HostName gabe.is-a-linux-user.org
    User gabe
    IdentityFile ~/.ssh/id_ed25519

Host laptop
    HostName gabe.is-a-linux-user.org
    User gabe
    IdentityFile ~/.ssh/id_ed25519
    Port 5022

Host vps
    HostName vps.gabedunn.dev
    User ubuntu
    IdentityFile ~/.ssh/id_ed25519


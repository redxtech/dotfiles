#!/usr/bin/env sh

# pull in some dependencies
# shellcheck source=../utils/testdeps.sh
. "$HOME/.local/bin/utils/testdeps.sh" || exit 1

# function to make a point that you're not in vim
main () {
    # test to see if all the dependencies are installed
    if ! testdeps cowsay; then
        exit 1
    fi

   cowsay "You're not in vim idiot, you're in your shell!"
}

main

